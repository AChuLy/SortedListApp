<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="183" Width="447">
    <Grid>
        <Button x:Name="add" Content="Add" HorizontalAlignment="Left" Height="41" Margin="267,58,0,0" VerticalAlignment="Top" Width="131" Click="add_Click"/>
        <TextBox x:Name="key" HorizontalAlignment="Left" Height="17" Margin="81,58,0,0" Text="Key" TextWrapping="Wrap" VerticalAlignment="Top" Width="162" />
        <TextBox x:Name="value" HorizontalAlignment="Left" Height="17" Margin="81,87,0,0" Text="Value" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>

    </Grid>
</Window>
